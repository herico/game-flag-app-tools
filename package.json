{
  "name": "game-flag-app",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "description": "Flag quiz PWA tools and scripts",
  "scripts": {
  "fetch:json": "node ./tools/fetch-assets.mjs --only=json",
    "fetch:svg": "node ./tools/fetch-assets.mjs --svg",
    "fetch:png": "node ./tools/fetch-assets.mjs --png",
    "fetch:flags": "node ./tools/fetch-assets.mjs --both",
    "fetch:sample": "node ./tools/fetch-assets.mjs --both --limit 30"
  }
}
